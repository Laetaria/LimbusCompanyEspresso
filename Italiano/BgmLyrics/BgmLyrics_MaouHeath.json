    {
      "id": 0,
      "content": "<style=\"den_CP6_2\">Quanto ho desiderato un’altro",
      "vocalid": 0
    },
    {
      "id": 1,
      "content": "<style=\"den_CP6_2\">Futuro migliore più allegro felice",
      "vocalid": 0
    },
    {
      "id": 2,
      "content": "<style=\"den_CP6_2\">Eccomi qui in piedi al cancello",
      "vocalid": 0
    },
    {
      "id": 3,
      "content": "<style=\"den_CP6_2\">Cari vecchi tempi",
      "vocalid": 0
    },
    {
      "id": 4,
      "content": "<style=\"den_CP6_2\">Mi sono deciso",
      "vocalid": 0
    },
    {
      "id": 5,
      "content": "<style=\"den_CP6_2\">Quanto avrai sofferto per la mia rabbia",
      "vocalid": 0
    },
    {
      "id": 6,
      "content": "<style=\"den_CP6_2\">I modi volgari goffi",
      "vocalid": 0
    },
    {
      "id": 7,
      "content": "Quanto devi aver ignorato la tua meraviglia",
      "vocalid": 1
    },
    {
      "id": 8,
      "content": "I modi diretti sinceri",
      "vocalid": 1
    },
    {
      "id": 9,
      "content": "<style=\"den_CP6_2\">Eccoci qui alla terra destinata",
      "vocalid": 0
    },
    {
      "id": 10,
      "content": "<style=\"den_CP6_2\">Vi metterò fine",
      "vocalid": 0
    },
    {
      "id": 11,
      "content": "Osserverai la mia fine?",
      "vocalid": 1
    },
    {
      "id": 12,
      "content": "<style=\"den_CP6_2\">Oh",
      "vocalid": 0
    },
    {
      "id": 13,
      "content": "<style=\"den_CP6_2\">Giriamo e giriamo",
      "vocalid": 0
    },
    {
      "id": 14,
      "content": "<style=\"den_CP6_2\">Stringendo il dolore",
      "vocalid": 0
    },
    {
      "id": 15,
      "content": "<style=\"den_CP6_2\">Spinti dai nostri eghi",
      "vocalid": 0
    },
    {
      "id": 16,
      "content": "<style=\"den_CP6_2\">Sentimenti inespressi",
      "vocalid": 0
    },
    {
      "id": 17,
      "content": "<style=\"den_CP6_2\">Intessendo le nostre versioni del passato",
      "vocalid": 0
    },
    {
      "id": 18,
      "content": "<style=\"den_CP6_2\">Che non potevi mai conoscere",
      "vocalid": 0
    },
    {
      "id": 19,
      "content": "<style=\"den_CP6_2\">Perchè so",
      "vocalid": 0
    },
    {
      "id": 20,
      "content": "<style=\"den_CP6_2\">Devo essere la ragione per cui",
      "vocalid": 0
    },
    {
      "id": 21,
      "content": "<style=\"den_CP6_2\">Hai rinunciato ai tuoi sorrisi",
      "vocalid": 0
    },
    {
      "id": 22,
      "content": "<style=\"den_CP6_2\">E la speranza nei tuoi occhi fu rubata",
      "vocalid": 0
    },
    {
      "id": 23,
      "content": "<style=\"den_CP6_2\">Devo essere la ragione per cui",
      "vocalid": 0
    },
    {
      "id": 24,
      "content": "<style=\"den_CP6_2\">Devi dirmi tutte queste bugie",
      "vocalid": 0
    },
    {
      "id": 25,
      "content": "<style=\"den_CP6_2\">Ti auguro una vita migliore senza me al tuo fianco",
      "vocalid": 0
    },
    {
      "id": 26,
      "content": "<style=\"den_CP6_2\">Il messaggio è già detto attraverso chiazze viola",
      "vocalid": 0
    },
    {
      "id": 27,
      "content": "<style=\"den_CP6_2\">Quanto ci saremo discostati",
      "vocalid": 0
    },
    {
      "id": 28,
